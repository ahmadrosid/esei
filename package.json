{
  "private": true,
  "name": "esei",
  "description": "",
  "author": "ahmadrosid",
  "version": "0.1.0",
  "scripts": {
    "build": "yarn build:css && remix build",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "dev": "concurrently \"yarn dev:css\" \"remix dev\"",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "postinstall": "remix setup vercel",
    "lint": "eslint app",
    "lint:fix": "eslint app --fix && yarn format",
    "type-check": "tsc --noEmit",
    "format": "prettier --write app",
    "up-latest": "yarn upgrade-interactive --latest",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "start": "remix-serve build",
    "prepare": "husky install",
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@prisma/client": "^4.2.1",
    "@remix-run/react": "^1.6.7",
    "@remix-run/serve": "^1.6.7",
    "@remix-run/server-runtime": "^1.7.0",
    "@remix-run/vercel": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "remix": "^1.6.7",
    "remix-auth": "^3.2.2",
    "remix-auth-form": "^1.1.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/cz-commitlint": "^16.3.0",
    "@remix-run/dev": "^1.6.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^18.7.13",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.8",
    "commitizen": "^4.2.5",
    "commitlint": "^16.3.0",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "eslint": "8.11.0",
    "eslint-config-sznm": "^0.2.1",
    "husky": "^7.0.0",
    "lint-staged": "^12.5.0",
    "postcss": "^8.4.16",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
